CREATE Procedure [dbo].[CreateUser]
@name varchar(50),
@email nvarchar(128),
@userName [nvarchar](256),
AS
BEGIN
	INSERT INTO [dbo].[CreateUser]
           ([Name]
           ,[Email]
           ,[UserName]
	 VALUES (@name, @email, @userName)
END
GO
