/****** Object:  Database [KaizanDB]    Script Date: 8/15/2018 2:09:06 PM ******/
CREATE DATABASE [KaizanDB]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'KaizanDB', FILENAME = N'F:\Data\KaizanDB.mdf' , SIZE = 4096KB , MAXSIZE = UNLIMITED, FILEGROWTH = 1024KB )
 LOG ON 
( NAME = N'KaizanDB_log', FILENAME = N'F:\Log\KaizanDB_log.ldf' , SIZE = 1024KB , MAXSIZE = 2048GB , FILEGROWTH = 10%)
GO
ALTER DATABASE [KaizanDB] SET COMPATIBILITY_LEVEL = 120
GO
IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [KaizanDB].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO
